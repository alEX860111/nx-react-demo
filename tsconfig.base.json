{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2019", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "strictPropertyInitialization": false,
    "baseUrl": ".",
    "paths": {
      "@nx-react-demo/data-access-todo": ["libs/data-access-todo/src/index.ts"],
      "@nx-react-demo/data-access-user": ["libs/data-access-user/src/index.ts"],
      "@nx-react-demo/feature-todo": ["libs/feature-todo/src/index.ts"],
      "@nx-react-demo/feature-user": ["libs/feature-user/src/index.ts"],
      "@nx-react-demo/util-di": ["libs/util-di/src/index.ts"]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
