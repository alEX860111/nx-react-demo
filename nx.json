{
  "npmScope": "nx-react-demo",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "projects": {
    "data-access-todo": {
      "tags": ["type:data-access"]
    },
    "data-access-user": {
      "tags": ["type:data-access"]
    },
    "feature-todo": {
      "tags": ["type:feature"]
    },
    "feature-user": {
      "tags": ["type:feature"]
    },
    "my-app": {
      "tags": ["type:app"]
    },
    "my-app-e2e": {
      "tags": [],
      "implicitDependencies": ["my-app"]
    },
    "util-data-access": {
      "tags": ["type:util"]
    }
  }
}
